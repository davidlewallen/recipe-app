{
  "engines": {
    "node": "8.5.0",
    "npm": "5.3.0"
  },
  "name": "recipe-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-bootstrap": "^0.32.13",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^4.3.0",
    "autoprefixer": "7.1.6",
